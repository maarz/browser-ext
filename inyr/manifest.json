{
    "manifest_version": 2,
    "name": "INYR autofill",
    "version": "1.0",
    "description": "A simple extension with a button that executes a custom script.",
    "browser_action": {
      "default_icon": {
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
      }
    },
    "permissions": [
      "activeTab"
    ],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
  }